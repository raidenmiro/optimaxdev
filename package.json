{
  "name": "optimax-dev",
  "packageManager": "@pnpm/pnpm@7.8.0",
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "lefthook install",
    "dev": "vite",
    "build": "tsc && vite build",
    "commit": "cz",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint ./src/**/*.css",
    "format": "prettier ./src/**/*.{ts,tsx,css} --write",
    "sprite": "sprite --root assets -o public/sprite -d src/view/icon/sprite.h.ts",
    "colors": "npx convert-to-oklch ./src/**/*.css",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@csstools/postcss-oklab-function": "^2.2.2",
    "@logux/stylelint-config": "^0.15.1",
    "@neodx/svg": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "convert-to-oklch": "^1.2.3",
    "cz-emoji": "1.3.2-canary.2",
    "eslint": "^8.41.0",
    "eslint-kit": "^9.0.9",
    "lefthook": "^1.4.1",
    "msw": "^1.2.1",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-nesting": "^11.2.2",
    "postcss-opacity-percentage": "^2.0.0",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "react-redux": "^8.0.5",
    "stylelint": "^15.6.2",
    "stylelint-plugin-logical-css": "^0.12.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
