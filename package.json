{
  "name": "optimax-dev",
  "packageManager": "@pnpm/pnpm@7.8.0",
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "lefthook install",
    "dev": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "build": "tsc && vite build",
    "commit": "cz",
    "lint": "tsc --noEmit && eslint ./src && stylelint ./src/**/*.css",
    "format": "prettier ./src/**/*.{ts,tsx,css} --write",
    "sprite": "sprite --root assets -o public/sprite -d src/shared/view/icon/sprite.h.ts",
    "colors": "npx convert-to-oklch ./src/**/*.css",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "classcat": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@csstools/postcss-oklab-function": "^2.2.2",
    "@logux/stylelint-config": "^0.15.1",
    "@neodx/svg": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/ui": "^0.31.2",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "convert-to-oklch": "^1.2.3",
    "cz-emoji": "1.3.2-canary.2",
    "eslint": "^8.41.0",
    "eslint-kit": "^9.0.9",
    "jsdom": "^22.1.0",
    "keen-slider": "^6.8.5",
    "lefthook": "^1.4.1",
    "msw": "^1.2.1",
    "nanoid": "^4.0.2",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-nesting": "^11.2.2",
    "postcss-opacity-percentage": "^2.0.0",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "react-redux": "^8.0.5",
    "storybook": "^7.0.18",
    "stylelint": "^15.6.2",
    "stylelint-plugin-logical-css": "^0.12.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}